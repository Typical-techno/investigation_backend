generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       String @id @default(uuid())
    username String @unique
    password String
    desigination String
    police_thana String
    email String @unique
    isActive Status @default(Active)
}

enum Status {
    Active
    Inactive
}
